{
	"name": "heroyt/laser-arena-control",
	"description": "Systém pro lepší zpracování a sjednocení výsledků LaserGame z různých arén a různých výrobců.",
	"type": "project",
	"version": "0.2.2",
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.laserliga.cz"
		}
	],
	"scripts": {
		"serve": "php -S localhost:8000 index.php",
		"test": "./vendor/bin/phpunit",
		"testUnit": "./vendor/bin/phpunit --testsuite=unit",
		"testIntegration": "./vendor/bin/phpunit --testsuite=integration",
		"docs": "doxygen",
		"build-production": "composer update --no-dev && composer dump-autoload --no-dev && npm install && npm run buildDev",
		"build": "composer update && composer dump-autoload && npm install && npm run build"
	},
	"require": {
		"dibi/dibi": "^4.2",
		"ext-curl": "*",
		"ext-fileinfo": "*",
		"ext-gettext": "*",
		"ext-mbstring": "*",
		"ext-simplexml": "*",
		"ext-sockets": "*",
		"ext-zip": "*",
		"gettext/gettext": "^5.5",
		"guzzlehttp/guzzle": "^7.4",
		"latte/latte": "^3.0.0",
		"nette/di": "^3.0",
		"nette/http": "^3.1",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"php": "^8.1",
		"psr/log": "^3.0",
		"tracy/tracy": "^2.8",
		"endroid/qr-code": "^4.4",
		"ext-pcntl": "*",
		"lsr/interfaces": "^0.1",
		"lsr/helpers": "^0.1",
		"lsr/core": "^0.1",
		"lsr/routing": "^0.1",
		"lsr/logging": "^0.1",
		"lsr/request": "^0.1"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5"
	},
	"license": "GPL-3.0-or-later",
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		},
		"files": [
			"include/functions.php"
		]
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	]
}
