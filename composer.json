{
	"name": "heroyt/laser-arena-control",
	"type": "project",
	"scripts": {
		"serve": "php -S localhost:8000 index.php",
		"test": "./vendor/bin/phpunit",
		"docs": "doxygen",
		"build-production": "composer update --no-dev && composer dump-autoload --no-dev && npm install && npm run buildDev",
		"build": "composer update && composer dump-autoload && npm install && npm run build"
	},
	"require": {
		"dibi/dibi": "^4.2",
		"ext-curl": "*",
		"ext-fileinfo": "*",
		"ext-gettext": "*",
		"ext-mbstring": "*",
		"ext-simplexml": "*",
		"ext-sockets": "*",
		"ext-zip": "*",
		"gettext/gettext": "^5.5",
		"guzzlehttp/guzzle": "^7.4",
		"latte/latte": "^2.10",
		"nette/di": "^3.0",
		"nette/http": "^3.1",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"php": "^8.1",
		"psr/log": "^1.1",
		"tracy/tracy": "^2.8",
		"endroid/qr-code": "^4.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5"
	},
	"license": "GPL-3.0",
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		},
		"files": [
			"include/functions.php"
		]
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	]
}
