msgctxt "screen.trigger"
msgid "Obrazovka se zobrazuje jako výchozí, pokud žádná jiná před ní nesplňuje podmínku."
msgstr "The screen is displayed as default if no other screen before it meets the condition."

msgctxt "screen.trigger"
msgid "Výchozí"
msgstr "Default"

msgctxt "screen.trigger"
msgid "Obrazovka se zobrazí, pokud je poslední hra načtená, ale nespuštěná."
msgstr "The screen will appear if the last game is loaded but not started."

msgctxt "screen.trigger"
msgid "Hra načtena"
msgstr "Game loaded"

msgctxt "screen.trigger"
msgid "Obrazovka se zobrazí, pokud poslední hra právě probíhá."
msgstr "The screen will appear if the last game is currently in progress."

msgctxt "screen.trigger"
msgid "Hra hraje"
msgstr "Game is playing"

msgctxt "screen.trigger"
msgid "Obrazovka se zobrazí, pokud poseldní hra už skončila."
msgstr "The screen will display if the last game has already ended."

msgctxt "screen.trigger"
msgid "Hra skončila"
msgstr "Game ended"

msgctxt "screen.trigger"
msgid "Obrazovka se zobrazí, pokud se vyvolá nějaká manuální událost."
msgstr "The screen will appear if a manual event is triggered."

msgctxt "screen.trigger"
msgid "Vlastní událost"
msgstr "Custom event"

